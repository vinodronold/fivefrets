{
  "name": "fivefrets",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "^1.0.10",
    "react-test-renderer": "^15.6.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "material-components-web": "^0.16.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "youtube-player": "^5.4.0"
  },
  "scripts": {
    "build-css": "node-sass src/app/styles/ -o src/app/styles/ --include-path=node_modules",
    "watch-css": "npm run build-css && node-sass src/app/styles/ -o src/app/styles/ --include-path=node_modules --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
